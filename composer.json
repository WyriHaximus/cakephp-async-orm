{
  "name": "wyrihaximus/cake-async-orm",
  "description": "Asynchronous CakePHP ORM",
  "type":"cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6||^7.0",
    "cakephp/cakephp": "^3.0.0",
    "wyrihaximus/react-child-process-pool": "^1.3",
    "doctrine/annotations": "^1.2",
    "phpdocumentor/reflection-docblock": "^2.0|^3.0",
    "nikic/php-parser": "^2.0 || ^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5||^4.8",
    "vectorface/dunit": "~2.0",
    "squizlabs/php_codesniffer": "~1.5.6"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\React\\Cake\\Orm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\React\\Tests\\Cake\\Orm\\": "tests/",
      "WyriHaximus\\React\\TestApp\\Cake\\Orm\\": "test_app/"
    }
  },
  "config": {
    "platform": {
      "php": "5.6"
    },
    "sort-packages": true
  }
}
