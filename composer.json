{
  "name": "wyrihaximus/cake-async-orm",
  "type":"cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^5.5||^7.0",
    "cakephp/cakephp": "^3.0.0",
    "wyrihaximus/react-child-process-pool": "^1.0.0",
    "doctrine/annotations": "^1.2",
    "phpdocumentor/reflection-docblock": "^2.0|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0||^5.0",
    "phake/phake": "^2.0",
    "vectorface/dunit": "~2.0",
    "squizlabs/php_codesniffer": "~1.5.6"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\React\\Cake\\Orm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\React\\Tests\\Cake\\Orm\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": 5.6
    }
  }
}
